  "components": {
    "schemas": {
      "BookingDto": {
        "type": "object",
        "properties": {
          "bookingId": {
            "type": "string",
            "format": "uuid"
          },
          "userId": {
            "type": "string",
            "format": "uuid"
          },
          "tripId": {
            "type": "string",
            "format": "uuid"
          },
          "bookingDate": {
            "type": "string",
            "format": "date-time"
          },
          "status": {
            "type": "string"
          },
          "pricePaid": {
            "pattern": "^-?(?:0|[1-9]\\d*)(?:\\.\\d+)?$",
            "type": [
              "number",
              "string"
            ],
            "format": "double"
          }
        }
      },
      "BusDto": {
        "type": "object",
        "properties": {
          "busId": {
            "type": "string",
            "format": "uuid"
          },
          "operator": {
            "type": "string"
          },
          "type": {
            "type": "string"
          },
          "capacity": {
            "pattern": "^-?(?:0|[1-9]\\d*)$",
            "type": [
              "integer",
              "string"
            ],
            "format": "int32"
          },
          "model": {
            "type": "string"
          },
          "modelYear": {
            "pattern": "^-?(?:0|[1-9]\\d*)$",
            "type": [
              "integer",
              "string"
            ],
            "format": "int32"
          },
          "driverName": {
            "type": "string"
          },
          "features": {
            "type": "string"
          }
        }
      },
      "City": {
        "required": [
          "nameEn",
          "nameAr"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "nameEn": {
            "type": "string"
          },
          "nameAr": {
            "type": "string"
          },
          "areas": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Station"
            }
          }
        }
      },
      "CreateBookingDto": {
        "required": [
          "userId",
          "tripId"
        ],
        "type": "object",
        "properties": {
          "userId": {
            "type": "string",
            "format": "uuid"
          },
          "tripId": {
            "type": "string",
            "format": "uuid"
          },
          "bookingDate": {
            "type": "string",
            "format": "date-time"
          },
          "pricePaid": {
            "pattern": "^-?(?:0|[1-9]\\d*)(?:\\.\\d+)?$",
            "type": [
              "number",
              "string"
            ],
            "format": "double"
          },
          "status": {
            "type": "string"
